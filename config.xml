<?xml version='1.0' encoding='utf-8'?>
<widget
  xmlns       = "http://www.w3.org/ns/widgets"
  xmlns:gap   = "http://phonegap.com/ns/1.0"
  id          = "com.storemobilizer.products-db"
  version     = "15.04.07"
  versionCode = "150407"
>
  <name>ProductsDB StoreMobilizer Demo</name>
  <author
    href="http://storemobilizer.com"
    email="info@storemobilizer.com">StoreMobilizer
  </author>
  <description></description>
  <preference name="phonegap-version" value="3.7.0"/>
  <preference name="fullscreen" value="true"/>
  <preference name="permissions" value="none"/>
  <feature    name="http://api.phonegap.com/1.0/file"/>

  <icon         src="icon.png" gap:platform="android" gap:qualifier="hdpi"/>
  <gap:splash   src="logo.png" gap:platform="android"/>
  <gap:platform name="android"/>
  <gap:plugin   name="org.apache.cordova.file"          version="1.3.1"/>
  <gap:plugin   name="org.apache.cordova.globalization" version="0.3.1"/>
  <gap:plugin   name="org.apache.cordova.inappbrowser"  version="0.5.2"/>
  <gap:plugin   name="org.apache.cordova.statusbar"     version="0.1.4"/>
  <access       origin="*"/>
</widget>